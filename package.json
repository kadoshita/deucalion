{
  "name": "deucalion",
  "version": "0.0.1",
  "description": "Alert management system for prometheus.",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev ./src/main.ts",
    "start": "node ./build/main.js",
    "test": "jest --config jest.config.json",
    "lint": "eslint ./src/ ./__tests__/ --ext .ts",
    "fix": "eslint ./src/ ./__tests__/ --ext .ts --fix",
    "format": "prettier --write ./**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadoshita/deucalion.git"
  },
  "author": "",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/kadoshita/deucalion/issues"
  },
  "homepage": "https://github.com/kadoshita/deucalion#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fastify": "^3.20.2",
    "node-fetch": "^2.6.1"
  }
